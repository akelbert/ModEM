# include Makefile.local
# OBJDIR = ./objs/3D_MT/IFortDebug
# F90 = ifort
# FFLAGS = -O3
# FFLAGS = -debug all -check bounds
# MODULE = -module $(OBJDIR)
# LIBS = -lblas -llapack
